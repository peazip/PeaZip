      if loadthemefile(s)<>0 then
         begin
         theme_failure;
         exit;
         end;
      except
         theme_failure;
         exit;
      end;
      end;
   5: begin
      OpenDialogTheme.initialdir:=sharepath+'themes'+directoryseparator;
