if Form_peach.SaveDialogPJ.Execute then
   if Form_peach.SaveDialogPJ.FileName<>'' then
      begin
      assignfile(t,Form_peach.SaveDialogPJ.FileName);
      rewrite(t);
      //if jobdefenc=1 then write_header(t);
      write(t,cl);
      closefile(t);
      end;
end;

