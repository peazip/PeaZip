   if Form_gwrap.visible=true then
      begin
      Form_gwrap.ButtonPause.enabled:=true;
      Form_gwrap.ButtonStop.enabled:=true;
      Form_gwrap.ButtonStopAll.enabled:=true;
      Form_gwrap.ShapeProgress.Width:=dummyprogressg;
